{
  "suggest.languageSourcePriority": 100,
  "coc.source.around.priority": 20,
  "coc.source.buffer.priority": 10,
  "yank.priority": 15,
  "coc.source.word.priority": 0,

  "diagnostic.virtualText": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate":true,

  "coc.preferences.formatOnSaveFiletypes": ["dart", "css", "Markdown", "javascript", "typescript", "vue", "ruby", "python"],
  "coc.preferences.rootPatterns": [".git", ".hg", ".projections.json"],

  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",

  "solargraph.formatting": true,
  "solargraph.diagnostics": true,
  "solargraph.useBundler": true,

  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "typescript", "vue"],

  "yank.highlight.duration": 200,
  "python.formatting.provider": "black",
  "python.venvPath": ".venv",

  "flutter.lsp.debug": true
}
