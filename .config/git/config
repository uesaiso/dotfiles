[init]
  templatedir = ~/.git_template
[push]
  default = current
[color]
  ui = auto
[alias]
  aa = add --all
  au = add -u
  co = checkout
  c = commit -v
  d = diff
  s = status -sb
[core]
  autocrlf = input
[pull]
  ff = only
[commit]
  template = ~/.gitmessage
[fetch]
  prune = true
[rebase]
  autosquash = true
[include]
  path = ~/.gitconfig.local
[diff]
  colorMoved = zebra
[user]
	name = uesaiso
	email = ysmsuehr@gmail.com
[credential]
	helper = osxkeychain
